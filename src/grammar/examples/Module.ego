module Module

import {
  Props { A }
  STL::Annotations::InstanceOf as InstanceOf
}

[InstanceOf]
public class B: A, C {
  public () {
    super(10)
  }

  u32 Prop: 100 {
      get => field
      set {
          print("prop set")
          field: value
      }
  }

  hello() {
    print("Hello ${Prop::field}")
  }

  i32 main(List<string> args) {
    hello()
    this.hello()
    this->hello()
    (@this).hello()
    => 0
  }
}
